---
import type { Props as $, List } from "astro-portabletext/types";
import { List as DefaultList } from "astro-portabletext/components";

export type Props = $<List>;

const props = Astro.props;
const listItemIs = (listItem: string) => listItem === props.node.listItem;

const Cmp = DefaultList;
---

<Cmp {...props}><slot /></Cmp>
